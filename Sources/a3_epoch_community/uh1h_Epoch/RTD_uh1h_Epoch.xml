          B1m = degToRad(B1m),
          Th0m = degToRad(Th0m),
          Th0t = degToRad(Th0t),
          mainRotorBrake = rotorBrake,
          profondeur = lonCyclic*5

        </Expression>        
        
      </MathExpression>      

